{
  "name": "profile-builder",
  "version": "1.0.0",
  "description": "Welcome to the Profile Builder project! This project is an internal initiative developed by the Talpods team...",
  "main": "index.js",
  "scripts": {
    "test:usermanager": "cd ./user-manager && npm test",
    "test:profilemanager": "cd ./profile-manager && npm test",
    "test:auth": "cd ./auth && npm test",
    "test:profileworker": "cd ./profile-worker && npm test",
    "test": "npm run test:usermanager && npm run test:profilemanager && npm run test:auth && npm run test:profileworker",
    "prettier": "npx prettier --write .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .prettierignore",
    "prepare": "husky install && chmod ug+x .husky/*"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "**/*.{ts,js,jsx,tsx,html,css,scss,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0"
  },
  "keywords": []
}
